{
    "manifest_version": 3,
    "name": "Line Writer",
    "version": "1.0.0",
    "description": "A notepad for your new tab page.",
    "icons": {
        "16": "./favicons/favicon-16x16.png",
        "32": "./favicons/favicon-32x32.png",
        "48": "./favicons/favicon-48x48.png",
        "128": "./favicons/favicon-128x128.png"
    },
    "chrome_url_overrides": {
        "newtab": "./index.html"
    },
    "permissions": [
        "storage"
    ]
}